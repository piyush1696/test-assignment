<section class="photos-content">
    <ul class="list-inline">
        {{#photos}}<li class="thumbnail">
            <div class="photo-thumbnail">
                <img src="{{.}}" class="img-responsive photo" data-toggle="modal" data-target="#selected-photo">
            </div>
        </li>{{/photos}}
    </ul>
    <div class="modal fade" tabindex="-1" role="dialog" id="selected-photo"></div>
</section>
